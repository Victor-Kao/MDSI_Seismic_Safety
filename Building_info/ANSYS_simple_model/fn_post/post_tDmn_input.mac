    PLVAR, 40*i_storey+1, 103
    !/INPUT,'Save_Plot','mac'
        *DIM,Disp_Center_Y_%i_storey%_l%l%_b%b%,ARRAY,ntime,2,1
        *do,itime,1,ntime,1  
            *GET,Disp_Center_Y_%i_storey%_l%l%_b%b%(itime,1),VARI,1,REAL,itime*t_val_Incr
